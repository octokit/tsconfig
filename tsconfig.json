{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Octokit TSConfig",

  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2022",
    "lib": ["es2022"],
    "resolveJsonModule": true,
    "verbatimModuleSyntax": true
  }
}
